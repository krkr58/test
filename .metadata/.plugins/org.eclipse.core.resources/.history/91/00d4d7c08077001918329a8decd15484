
public class Person {
	public String name=null;
	public int age=0;

	public Person(){
		this.name=name;
		this.age=age;
		System.out.println(this.name);
		System.out.println(this.age);
	}

	public Person(String name,int age){
		this.name=name;
		this.age=age;
		System.out.println(this.name);
		System.out.println(this.age);
	}
}
